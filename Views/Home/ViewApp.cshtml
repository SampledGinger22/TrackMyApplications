@{
    ViewData["Title"] = "View Application";
}


<div class="content_body">
    @{
        Application tempApp = new Application();
        tempApp.Notes = @ViewBag.application.Notes;
        tempApp.Pros = @ViewBag.application.Pros;
        tempApp.Cons = @ViewBag.application.Cons;
        tempApp.AddContacts = @ViewBag.application.AddContacts;
        tempApp.BusinessName = @ViewBag.application.BusinessName;
        tempApp.JobTitle = @ViewBag.application.JobTitle;
    }
<partial name="_ViewApp" model="@tempApp"/>
</div>
<div class="d-flex flex-row justify-content-center" style="gap: 20px">
<partial name="_intForm"/>
@{
    Interview tempInt = new Interview();
    @if(ViewBag.nextInt != null)
    {
        tempInt.IntDate = @ViewBag.nextInt.IntDate;
        tempInt.Title = @ViewBag.nextInt.Title;
        tempInt.Notes = @ViewBag.nextInt.Notes;
    }
}
<partial name="_nextInt" model="@tempInt"/>

</div>

@{
List<Interview> interviews = new List<Interview>();
interviews = @ViewBag.interviews;
}

<partial name="_pastInts" model="@interviews"/>
<script src="~/js/phones.js"></script>
<script src="~/js/currency.js"></script>